function initCmc(cmcConfig) {
  // add cmc info
  for (const [ticker, config] of Object.entries(cryptoConfig)) {
    if (cmcConfig[ticker] == null) {
      continue;
    }
    config.cmcId = cmcConfig[ticker].id;
    config.holderIgnoreList = cmcConfig[ticker].holderIgnoreList || [];
  }
}
